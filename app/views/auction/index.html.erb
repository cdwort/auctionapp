<div class="row">
<ul class="large-block-grid-4">
  <% @auction_items.each do |item| %>
    <li>
    <div class="panel">
      <%= link_to "#{item.name}", auction_item_path(item) %>
      <br />
      <%= link_to image_tag("#{item.thumbnail_url}"), auction_item_path(item) %>
      <%= item.end_time %>
    </div>
    </li>
  <% end %>
</ul>
</div>
